{
    "name": "gecche/laravel-foorm",
    "description": "Building laravel forms for ORM tasks in a easy way",
    "keywords": ["laravel", "form", "ORM"],
    "license": "MIT",
    "authors": [
        {
            "name": "Giacomo Terreni",
            "email": "giacomo.terreni@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^6.0",
        "gecche/laravel-dbhelper": "2.0.*",
        "gecche/laravel-breeze": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "Gecche\\Foorm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gecche\\Foorm\\FoormServiceProvider"
            ],
            "aliases": {
                "Foorm": "Gecche\\Foorm\\Facades\\Foorm"
            }
        }
    }
}
